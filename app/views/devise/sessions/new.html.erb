<% content_for :fixed_width do %>Yes<% end %>

<%=
  args = {
    user: resource,
    errors: resource.errors,
    url: session_path(resource_name),
    remember_me: devise_mapping.rememberable?,
    password_url: new_password_path(resource_name),
    authenticity_token: form_authenticity_token
  }

  react_component('ReactLogIn', args, {prerender: @should_prerender, class: 'react-component'})
%>
